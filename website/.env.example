# EmailJS Configuration
# Get these credentials from https://www.emailjs.com/
# Copy this file to .env and replace with your actual credentials

VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Site Configuration
# Base URL for SEO and canonical URLs
# Optional: defaults to https://peleiniki.com in production
# Use http://localhost:5173 for local development if needed
VITE_SITE_URL=https://peleiniki.com
